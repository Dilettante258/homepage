---
export interface Props {
  techName: string;
}

const { techName } = Astro.props;
const tech = techName.toLowerCase();
const isReact = tech.includes("react");
const isAstro = tech.includes("astro");
const isTypeScript = tech.includes("typescript") || tech === "ts";
const isRust = tech.includes("rust");
const isNode = tech.includes("node");
const isJavaScript = (tech.includes("javascript") || tech === "js") && !isNode;
const isKubernetes = tech.includes("kubernetes") || tech.includes("k8s");
const isDocker = tech.includes("docker");
const isMonorepo = tech.includes("monorepo");
const isVSCode = tech.includes("vscode") || tech.includes("vs code");
const isHono = tech.includes("hono");
const isDrizzle = tech.includes("drizzle");
const isCloudflare = tech.includes("cloudflare");
const isWasm = tech.includes("wasm") || tech.includes("webassembly");
const shouldHide = isJavaScript;
const isFallback = ![
  isReact,
  isAstro,
  isTypeScript,
  isRust,
  isNode,
  isJavaScript,
  isKubernetes,
  isDocker,
  isMonorepo,
  isVSCode,
  isHono,
  isDrizzle,
  isCloudflare,
  isWasm,
].some(Boolean);
