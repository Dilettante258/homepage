{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist",
    "solid-js\/.*\/types.*"
  ],
  "compilerOptions": {
    "allowArbitraryExtensions": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "baseUrl": "src",
    "paths": {
      "@": [
        "src/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ]
    }
  }
}
